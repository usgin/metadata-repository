extends ./base

block page-title
	title Search
	
block styles
	link(rel="stylesheet/less", type="text/css", href="/static/css/search.less")
	link(rel="stylesheet", type="text/css", href="/static/css/jqueryui/jquery-ui-1.8.16.custom.css")
	
block scripts
	script(type="application/javascript", src="/static/js/jquery-ui-1.8.16.custom.min.js")
	script(type="application/javascript", src="/static/js/underscore-1.3.3.min.js")
	script(type="application/javascript", src="/static/js/backbone-0.9.2.min.js")
	script(type="application/javascript", src="/static/js/jade.js")
	script(type="application/javascript", src="/static/js/utils.js")
	{% autoescape off %}
	script(type="application/javascript")
		| term = #{term};
	{% endautoescape %}
	script(type="application/javascript", src="/static/js/models.js")
	script(type="application/javascript", src="/static/js/search-views.js")
	script(type="application/javascript", src="/static/js/search.js")
